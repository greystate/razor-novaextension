<?xml version="1.0" encoding="UTF-8"?>
<completions>
    <provider name="razor.vv-helpers">
        <syntax>razor</syntax>
        <selector>*</selector>
        <expression>(?&lt;=Helpers\.)[a-zA-Z0-9-_]*</expression>
        <set>razor.vokseverk.helpers</set>
    </provider>
    
    <set name="razor.vokseverk.helpers">
        <completion string="GetAssetsFolder()" />
        <completion string="GetAssetPath($[file])" />
        <completion string="GetSiteSetting(&quot;$[setting]&quot;)" />
    </set>
</completions>


